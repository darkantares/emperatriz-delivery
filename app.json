{
  "expo": {
    "name": "emperatriz-delivery",
    "slug": "emperatriz-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "emperatrizdelivery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "apiUrl": {
        "development": "http://192.168.100.24:5000",
        "production": "https://api.tiendasdominicanas.com.do",
        "EXPO_PUBLIC_API_URL_PROD": "https://api.tiendasdominicanas.com.do",
        "EXPO_PUBLIC_API_URL_DEV_IOS": "http://localhost:3000",
        "EXPO_PUBLIC_API_URL_DEV_ANDROID": "http://192.168.100.24:5000",
        "EXPO_PUBLIC_API_URL_DEFAULT": "http://192.168.100.24:5000"
      },
      "socketUrl": {
        "development": "http://192.168.100.22:5000",
        "production": "https://api.tiendasdominicanas.com.do"
      }
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1125"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A1125"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-audio", "expo-asset"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
