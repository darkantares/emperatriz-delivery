{
  "expo": {
    "name": "tiendas dominicanas delivery",
    "slug": "emperatriz-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tiendasdominicanasdelivery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "apiUrl": {
        "EXPO_PUBLIC_API_URL_PROD": "https://api.tiendasdominicanas.com.do",
        "EXPO_PUBLIC_API_URL_DEV_IOS": "http://localhost:3000",
        "EXPO_PUBLIC_API_URL_DEV_ANDROID": "http://192.168.100.24:5000",
        "EXPO_PUBLIC_API_URL_DEFAULT": "http://192.168.100.24:5000"
      },
      "socketUrl": {
        "development": "http://192.168.100.22:5000",
        "production": "https://api.tiendasdominicanas.com.do"
      },
      "router": {
        "root": "./app"
      },
      "eas": {
        "projectId": "d9114a6d-b3be-4b76-b734-ce5aefdc980c"
      }
    },
    "splash": {
      "image": "./assets/images/screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1125"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A1125"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.valentinrod.tiendasdominicanas"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./app"
        }
      ],
      "expo-audio",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "valentinrod"
  }
}
